    "multer": "^2.0.2",
    "nanoid": "^3.3.11",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.2",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
